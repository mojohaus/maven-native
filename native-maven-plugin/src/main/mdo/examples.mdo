<model>
  <id>association-test</id>
  <name>AssociationTest</name>
  <defaults>
    <default>
      <key>package</key>
      <value>org.codehaus.modello.association.package1</value>
    </default>
  </defaults>
  <classes>
  <!--
     <class rootElement="true" xml.tagName="root">
      <name>Root</name>
      <description/>
      <version>1.0.0</version>
      <fields>
        <field>
          <name>listSetMapProperties</name>
          <version>4.0.0</version>
          <description/>
          <association>
            <type>ListSetMapProperties</type>
          </association>
        </field>
      </fields>
    </class>
  -->    
    <class>
      <name>Person</name>
      <fields>
        <field>
          <name>location</name>
          <association>
            <type>Location</type>
          </association>
        </field>
      </fields>
    </class>
    
    <class>
      <name>Location</name>
      <packageName>org.codehaus.modello.association.package2</packageName>
      <fields>
        <field>
          <name>persons</name>
          <association>
            <type>Person</type>
            <multiplicity>*</multiplicity>
          </association>
        </field>
      </fields>
    </class>

    <!--
    |
    | List, Set, Map and Properties association tests.
    |
    |-->
    <class>
      <name>ListSetMapProperties</name>
      <version>4.0.0</version>
      <fields>
        <field>
          <name>list</name>
          <version>4.0.0</version>
          <type>List</type>
          <association>
            <type>Person</type>
            <multiplicity>*</multiplicity>
          </association>
        </field>
        <field>
          <name>set</name>
          <version>4.0.0</version>
          <type>Set</type>
          <association>
            <type>Person</type>
            <multiplicity>*</multiplicity>
          </association>
        </field>
        <field  >
          <name>map</name>
          <version>4.0.0</version>
          <type>Map</type>
          <association stash.keyType="String" >
            <type>Person</type>
            <multiplicity>*</multiplicity>
          </association>
        </field>
        <field>
          <name>properties</name>
          <version>4.0.0</version>
          <type>Properties</type>
          <association>
            <type>Person</type>
            <multiplicity>*</multiplicity>
          </association>
        </field>
      </fields>
    </class>
  </classes>
</model>
